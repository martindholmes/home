         <ref name="xh_model.measureLike_alternation"/>
         <ref name="xh_model.addressLike_alternation"/>
         <ref name="xh_model.nameLike_alternation"/>
      </choice>
   </define>
   <define name="xh_model.pPart.data_sequence">
      <ref name="xh_model.dateLike_sequence"/>
      <ref name="xh_model.measureLike_sequence"/>
      <ref name="xh_model.addressLike_sequence"/>
      <ref name="xh_model.nameLike_sequence"/>
   </define>
   <define name="xh_model.pPart.data_sequenceOptional">
      <optional>
         <ref name="xh_model.dateLike_sequenceOptional"/>
      </optional>
      <optional>
         <ref name="xh_model.measureLike_sequenceOptional"/>
      </optional>
      <optional>
         <ref name="xh_model.addressLike_sequenceOptional"/>
      </optional>
      <optional>
         <ref name="xh_model.nameLike_sequenceOptional"/>
      </optional>
   </define>
   <define name="xh_model.pPart.data_sequenceOptionalRepeatable">
      <zeroOrMore>
         <ref name="xh_model.dateLike_sequenceOptionalRepeatable"/>
      </zeroOrMore>
      <zeroOrMore>
         <ref name="xh_model.measureLike_sequenceOptionalRepeatable"/>
      </zeroOrMore>
      <zeroOrMore>
         <ref name="xh_model.addressLike_sequenceOptionalRepeatable"/>
      </zeroOrMore>
      <zeroOrMore>
         <ref name="xh_model.nameLike_sequenceOptionalRepeatable"/>
      </zeroOrMore>
   </define>
   <define name="xh_model.pPart.data_sequenceRepeatable">
      <oneOrMore>
         <ref name="xh_model.dateLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="xh_model.measureLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="xh_model.addressLike_sequenceRepeatable"/>
      </oneOrMore>
      <oneOrMore>
         <ref name="xh_model.nameLike_sequenceRepeatable"/>
      </oneOrMore>
   </define>
   <define name="xh_html">
      <element name="html" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The root html element for normal complete documents) The html element is only root element allowed for conventional HTML5 documents, but we also allow other block elements to serve as root for pragmatic reasons.</a:documentation>
         <group>
            <ref name="xh_head"/>
            <ref name="xh_body"/>
         </group>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.hasLang.attributes"/>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_head">
      <element name="head" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The HTML head element) The head element contains links to (external-only) CSS and JS, as well as a title element and meta tags. We impose an order here which is not enforced by the HTML5 standard, placing the single title element first, followed by meta tags, CSS, and script. One of the meta tags must be a charset specification.</a:documentation>
         <group>
            <ref name="xh_title"/>
            <optional>
               <ref name="xh_base"/>
            </optional>
            <oneOrMore>
               <ref name="xh_meta"/>
            </oneOrMore>
         </group>
      </element>
   </define>
   <define name="xh_title">
      <element name="title" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Page title.) The plain-text title of the page, as shown in browser title bars.</a:documentation>
         <text/>
      </element>
   </define>
   <define name="xh_meta">
      <element name="meta" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Header meta tag) Used for specifying character set, metadata, etc.</a:documentation>
         <empty/>
         <optional>
            <attribute name="charset">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Character set (use only UTF-8)) </a:documentation>
               <choice>
                  <value>UTF-8</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The only value we use is UTF-8.) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="xh_base">
      <element name="base" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Provides the base URL from which relative URLs are resolved.) The base element allows authors to specify the document base URL for the purposes of parsing URLs, and the name of the default navigable for the purposes of following hyperlinks. The element does not represent any content beyond this information.</a:documentation>
         <empty/>
         <ref name="xh_att.hasHref.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_body">
      <element name="body" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The principal container for page content as shown in the browser) In HTML5, the body element allows all flow content, but we are much more constrained here, allowing only block-level elements.</a:documentation>
         <group>
            <optional>
               <choice>
                  <ref name="xh_h1"/>
               </choice>
            </optional>
            <optional>
               <ref name="xh_aside"/>
            </optional>
            <choice>
               <oneOrMore>
                  <ref name="xh_div"/>
               </oneOrMore>
               <oneOrMore>
                  <ref name="xh_section"/>
               </oneOrMore>
               <ref name="xh_main"/>
               <ref name="xh_article"/>
            </choice>
            <optional>
               <ref name="xh_aside"/>
            </optional>
         </group>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_main">
      <element name="main" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The main element, containing block content) This is used as the root element for some content documents. This allows divs, dialogs, or articles in alternation.</a:documentation>
         <group>
            <ref name="xh_h2"/>
            <oneOrMore>
               <choice>
                  <ref name="xh_div"/>
                  <ref name="xh_dialog"/>
                  <oneOrMore>
                     <ref name="xh_article"/>
                  </oneOrMore>
               </choice>
            </oneOrMore>
         </group>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.hasTitle.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_article">
      <element name="article" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The principal container element, containing block content) This is the root element for article documents.</a:documentation>
         <group>
            <ref name="xh_h2"/>
            <oneOrMore>
               <choice>
                  <ref name="xh_macro.mhBlockContent"/>
                  <oneOrMore>
                     <ref name="xh_section"/>
                  </oneOrMore>
               </choice>
            </oneOrMore>
         </group>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.hasTitle.attributes"/>
         <optional>
            <attribute name="data-shortname">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(This is a simplified name of the poet used in the case of very long poet names.) </a:documentation>
               <data type="string"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="xh_aside">
      <element name="aside" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(A portion of a document whose content is only indirectly related to the document's main content) The aside element encloses content which is tangentially related to the content around it. It is used for callouts, footnotes/endnotes, and similar content.</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="xh_h4"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="xh_model.mhBlockBody"/>
            </zeroOrMore>
         </group>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.hasTitle.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_section">
      <element name="section" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(A subsection of a page) If using sections, there must be at least two.</a:documentation>
         <group>
            <optional>
               <ref name="xh_h3"/>
            </optional>
            <ref name="xh_macro.mhBlockContent"/>
         </group>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_div">
      <element name="div" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(A division of the footer element or the search page.) The div may be the main child of the main element.</a:documentation>
         <group>
            <optional>
               <ref name="xh_h3"/>
            </optional>
            <choice>
               <ref name="xh_p"/>
               <ref name="xh_div"/>
               <ref name="xh_macro.mhBlockContent"/>
            </choice>
         </group>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_h1">
      <element name="h1" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Level 1 heading) The h1(level 2 heading) element provides the heading for an entire page.</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="xh_macro.mhParaContent"/>
               <ref name="xh_br"/>
            </choice>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.hasTitle.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_h2">
      <element name="h2" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Level 2 heading) The h2 (level 2 heading) element provides the heading for a major subsection of a page.</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="xh_macro.mhParaContent"/>
               <ref name="xh_br"/>
            </choice>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.hasTitle.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_h3">
      <element name="h3" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Level 3 heading) The h3 (level 3 heading) element provides the heading for a secondary subsection of a page.</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="xh_macro.mhParaContent"/>
               <ref name="xh_br"/>
            </choice>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.hasTitle.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_h4">
      <element name="h4" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Level 4 heading) The h4 (level 4 heading) element provides the heading for a sub-sub-subsection of a page.</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="xh_macro.mhParaContent"/>
               <ref name="xh_br"/>
            </choice>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.hasTitle.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_p">
      <element name="p" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Prose paragraph.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="xh_macro.mhParaContent"/>
               <ref name="xh_br"/>
            </choice>
         </zeroOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_details">
      <element name="details" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Expandable details element with summary child.</a:documentation>
         <group>
            <ref name="xh_summary"/>
            <ref name="xh_macro.mhBlockContent"/>
         </group>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_summary">
      <element name="summary" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Single-line summary for details element.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_blockquote">
      <element name="blockquote" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Block quotation.</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="xh_p"/>
               <ref name="xh_ul"/>
               <ref name="xh_br"/>
               <ref name="xh_macro.mhParaContent"/>
            </choice>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_q">
      <element name="q" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Inline quotation. In rendering, quotation marks will be supplied, so do not include them.</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="xh_br"/>
               <ref name="xh_macro.mhParaContent"/>
            </choice>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_ul">
      <element name="ul" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Unordered list, usually bulleted.</a:documentation>
         <oneOrMore>
            <ref name="xh_li"/>
         </oneOrMore>
         <ref name="xh_att.classable.attribute.style"/>
         <optional>
            <attribute name="class">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The class attribute provides styles through rules in the CSS stylesheet.) </a:documentation>
               <list>
                  <oneOrMore>
                     <data type="token">
                        <param name="pattern">[^\p{C}\p{Z}]+</param>
                     </data>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="xh_ol">
      <element name="ol" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Ordered list, usually numbered.</a:documentation>
         <oneOrMore>
            <ref name="xh_li"/>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_li">
      <element name="li" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Item in a list or a menu.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="xh_ul"/>
               <ref name="xh_ol"/>
               <ref name="xh_p"/>
               <ref name="xh_macro.mhParaContent"/>
               <ref name="xh_br"/>
            </choice>
         </zeroOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_pre">
      <element name="pre" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Preformatted text which is to be presented exactly as written in the HTML file.</a:documentation>
         <text/>
      </element>
   </define>
   <define name="xh_span">
      <element name="span" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">General-purpose phrase-level element. Use only if there is no more specific alternative for what you want.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="xh_macro.mhParaContent"/>
               <ref name="xh_br"/>
            </choice>
         </zeroOrMore>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="mholmes.com-span-spansMustHaveNametypeStyleOrClass-constraint-rule-1">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xh="http://www.w3.org/1999/xhtml"
                      context="xh:span">
               <sch:assert test="@class or @style or @data-nametype"> 
                           A span element must have a class attribute, a data-nametype attribute, or a style attribute.
                        </sch:assert>
            </sch:rule>
         </pattern>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.hasLang.attributes"/>
         <ref name="xh_att.hasnametype.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_abbr">
      <element name="abbr" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Abbreviation) Any abbreviation, acronym, or similar term can be tagged with this element, and the <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">@title</code> attribute can be used to provide an expansion.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_address">
      <element name="address" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Address) Contains contact information for a person or organization related to the nearest ancestor <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;article&gt;</code> or <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;body&gt;</code> element.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_strong">
      <element name="strong" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Strong text) Text that needs to be highlighted because it is important; will be rendered by default in bold.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_code">
      <element name="code" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Inline computer code, filenames etc.) Inline text which is computer code or similar text such as identifiers.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_colgroup">
      <element name="colgroup" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Column group.) Defines a group of columns within a table.</a:documentation>
         <oneOrMore>
            <ref name="xh_col"/>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.hasSpan.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_col">
      <element name="col" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(A table column.) Specifies a table column within a <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;colgroup&gt;</code> element.</a:documentation>
         <empty/>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.hasSpan.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_img">
      <element name="img" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Image element. May be rendered inline or as a block, depending on where it appears in the document structure.</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="mholmes.com-img-imgWithoutCaptionNeedsAlt-constraint-rule-2">
            <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xh="http://www.w3.org/1999/xhtml"
                      context="xh:img">
               <sch:assert test="@alt or following-sibling::xh:figcaption"> 
                              If your img element is not inside a figure element with a figcaption, 
                              then it requires the @alt attribute to provide help for visually-impaired users.
                           </sch:assert>
            </sch:rule>
         </pattern>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.hasWidthAndHeight.attributes"/>
         <attribute name="src">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(path to the image (usually a relative path)) </a:documentation>
            <data type="anyURI">
               <param name="pattern">\S+</param>
            </data>
         </attribute>
         <optional>
            <attribute name="alt">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(description of the image for visually-impaired users. If not supplied, then figcaption will be used instead.) </a:documentation>
               <data type="string"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="title">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(description of the image which will pop up on mouseover. If not supplied, then figcaption will be used instead.) </a:documentation>
               <data type="string"/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="xh_source">
      <element name="source" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;source&gt;</code> HTML element specifies one or more media resources for the, <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;audio&gt;</code> and <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;video&gt;</code> elements.</a:documentation>
         <empty/>
         <attribute name="src">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Pointer to the source media file.) </a:documentation>
            <data type="anyURI">
               <param name="pattern">\S+</param>
            </data>
         </attribute>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The mime type specifies the applicable multimedia internet mail extension (MIME) media type) </a:documentation>
            <data type="token">
               <param name="pattern">[^\p{C}\p{Z}]+</param>
            </data>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="xh_audio">
      <element name="audio" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;audio&gt;</code> element provides an audio player to play an audio file specified in the child <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;source&gt;</code> element.</a:documentation>
         <oneOrMore>
            <ref name="xh_source"/>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <optional>
            <attribute name="controls">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>controls</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Specifies that default audio controls should be provided for the player.) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="xh_video">
      <element name="video" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">The <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;video&gt;</code> element provides a video player to play a video file specified in the child <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">&lt;source&gt;</code> element.</a:documentation>
         <oneOrMore>
            <ref name="xh_source"/>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.hasWidthAndHeight.attributes"/>
         <optional>
            <attribute name="controls">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>controls</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Specifies that default video controls should be provided for the player.) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="xh_canvas">
      <element name="canvas" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(An area for drawing graphics and animations) The canvas element is a locus for drawing operations. Alternative content may be provided in the form of a limited number of phrase-level elements. If <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">@width</code> and <code xmlns="http://www.w3.org/1999/xhtml"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0">@height</code> are not provided, they default to 300 and 150 respectively.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.hasWidthAndHeight.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_em">
      <element name="em" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Emphasized text; will be rendered by default in italics.</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="xh_macro.mhParaContent"/>
               <ref name="xh_br"/>
            </choice>
         </zeroOrMore>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_u">
      <element name="u" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Underlined text, in a context where we do not wish to conjecture why the underline is there (in an original article, or a quotation of an external source).</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="xh_macro.mhParaContent"/>
               <ref name="xh_br"/>
            </choice>
         </zeroOrMore>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_a">
      <element name="a" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.hasHref.attributes"/>
         <ref name="xh_att.hasnametype.attributes"/>
         <optional>
            <attribute name="title">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(explanation of what is being linked to. Will pop up when mousing over the link.) </a:documentation>
               <data type="string"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="target">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>_blank</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Open this link in a new window or tab.) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="xh_dialog">
      <element name="dialog" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">An HTML dialog element</a:documentation>
         <oneOrMore>
            <choice>
               <ref name="xh_p"/>
               <ref name="xh_figure"/>
               <ref name="xh_div"/>
            </choice>
         </oneOrMore>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_cite">
      <element name="cite" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">An HTML cite element</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_figure">
      <element name="figure" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A figure consists of an image and a caption.</a:documentation>
         <choice>
            <oneOrMore>
               <ref name="xh_p"/>
            </oneOrMore>
            <group>
               <ref name="xh_img"/>
               <optional>
                  <ref name="xh_figcaption"/>
               </optional>
            </group>
         </choice>
         <ref name="xh_att.classable.attribute.style"/>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.hasTabindex.attributes"/>
         <optional>
            <attribute name="class">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The class attribute provides styles through rules in the CSS stylesheet.) </a:documentation>
               <list>
                  <oneOrMore>
                     <choice>
                        <value>leftFloat</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Float this figure to the left and flow the text around it.) </a:documentation>
                        <value>center</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Centre this figure and do not float text around it.) </a:documentation>
                        <value>rightFloat</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Float this figure to the right and flow the text around it.) </a:documentation>
                        <value>lightbox-figure</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Used for a figure which appears in the ad gallery.) </a:documentation>
                     </choice>
                  </oneOrMore>
               </list>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="xh_br">
      <element name="br" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A hard-coded linebreak. Avoid unless you absolutely need it. Use it at the end of lines in poems.</a:documentation>
         <empty/>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_figcaption">
      <element name="figcaption" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Caption for an image in a figure element.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.hasTabindex.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_td">
      <element name="td" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A table data cell.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
      </element>
   </define>
   <define name="xh_button">
      <element name="button" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">An HTML button element.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <ref name="xh_att.classable.attributes"/>
         <ref name="xh_att.identifiable.attributes"/>
         <ref name="xh_att.hasTabindex.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="xh_th">
      <element name="th" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A table label or heading cell.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
         <optional>
            <attribute name="scope">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>col</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Scope of this heading is the column below it.) </a:documentation>
                  <value>row</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Scope of this heading is the row to the right of it.) </a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="xh_tr">
      <element name="tr" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A table row.</a:documentation>
         <oneOrMore>
            <zeroOrMore>
               <ref name="xh_th"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="xh_td"/>
            </zeroOrMore>
         </oneOrMore>
      </element>
   </define>
   <define name="xh_caption">
      <element name="caption" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A table caption.</a:documentation>
         <ref name="xh_macro.mhParaContent"/>
      </element>
   </define>
   <define name="xh_table">
      <element name="table" ns="http://www.w3.org/1999/xhtml">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A table.</a:documentation>
         <group>
            <optional>
               <ref name="xh_caption"/>
            </optional>
            <zeroOrMore>
               <ref name="xh_colgroup"/>
            </zeroOrMore>
            <oneOrMore>
               <ref name="xh_tr"/>
            </oneOrMore>
         </group>
         <ref name="xh_att.classable.attributes"/>
         <empty/>
      </element>
   </define>
