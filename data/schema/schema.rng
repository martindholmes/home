   <sch:ns xmlns="http://www.tei-c.org/ns/1.0"
           xmlns:rng="http://relaxng.org/ns/structure/1.0"
           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           xmlns:xh="http://www.w3.org/1999/xhtml"
           prefix="sch1x"
           uri="http://www.ascc.net/xml/schematron"/>
   <sch:ns xmlns="http://www.tei-c.org/ns/1.0"
           xmlns:sch="http://purl.oclc.org/dsdl/schematron"
           xmlns:xh="http://www.w3.org/1999/xhtml"
           uri="http://www.w3.org/1999/xhtml"
           prefix="xh"/>
   <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml">
      <sch:let name="smartDouble" value="'[ââ]'"/>
      <sch:let name="smartSingle" value="'[ââ]'"/>
      <sch:let name="straightDouble" value="'&#34;'"/>
      <sch:let name="straightApos" value="''''"/>
      <sch:let name="straightQuotes"
               value="concat('[', $straightDouble, $straightApos, ']')"/>
   </sch:pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mholmes.com-mh-noQuotesInText-constraint-rule-3">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml"
                context="xh:*[text()][not(ancestor-or-self::xh:script or ancestor-or-self::xh:style or ancestor-or-self::xh:code or ancestor-or-self::xh:pre)]">
         <sch:assert test="not(text()[matches(., $straightQuotes)])">
                           Do not use straight quotes in text. Use the q or 
                           span elements wherever possible; for apostrophes, 
                           use the smart version (â).
                        </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mholmes.com-mh-noBadImageFilenames-constraint-rule-4">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml"
                context="xh:img">
         <sch:assert test="matches(@src, '^[a-zA-Z0-9/\._\-]+$')">
                           Image file paths must be well-formed (no spaces or punctuation).
                        </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mholmes.com-mh-spacesAfterPeriods-constraint-rule-5">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml"
                context="xh:*[not(self::xh:code or self::xh:pre)][text()]">
         <sch:assert test="not(text()[matches(., '[^\.A-Z]\.[A-Z]')])">
                           Don't forget to put a space after every period.
                        </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mholmes.com-mh-noSpacedPeriodEllipses-constraint-rule-6">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml"
                context="xh:*[not(self::xh:code or self::xh:pre)][text()]">
         <sch:assert test="not(text()[matches(., '\.\s*\.\s*\.')])">
                           Don't use literal periods to create an ellipsis. Use the 
                           ellipsis character (â¦, Control + Shift + period) instead.
                        </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mholmes.com-mh-noSpacesInHrefs-constraint-rule-7">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml"
                context="xh:a">
         <sch:assert test="not(matches(@href, '\s+'))">
                           There should be no spaces in href attributes (links).
                        </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mholmes.com-mh-noLinksInLinks-constraint-rule-8">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml"
                context="xh:a">
         <sch:assert test="not(descendant::xh:a[@href])">
                           Don't place links inside other links.
                        </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mholmes.com-mh-periodsAndCommasOutsideLinks-constraint-rule-9">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml"
                context="xh:a[@href]">
         <sch:assert test="not(matches(., '[\.,:;]\s*$')) or matches(., '((Esq\.)|(A\.\s*W\.)|(\.\s*\.\s*\.))$')">
                           Don't include trailing periods and commas inside link text.
                        </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mholmes.com-mh-useProperEllipsis-constraint-rule-10">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml"
                context="xh:*[not(self::xh:code or self::xh:pre)][text()]">
         <sch:assert test="not(text()[matches(., '\.\.\.')])">
                           Use an ellipsis (â¦) rather than three periods.
                        </sch:assert>
      </sch:rule>
   </pattern>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="mholmes.com-mh-idShouldMatchFilename-constraint-rule-11">
      <sch:rule xmlns="http://www.tei-c.org/ns/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns:xh="http://www.w3.org/1999/xhtml"
                context="xh:article | xh:main | xh:html">
         <sch:let name="fname" value="concat(@id, '.xml')"/>
         <sch:assert test="ends-with(document-uri(/), $fname)">
                           ERROR: The @id of your document (<sch:value-of select="@id"/>) 
                           does not match the document file name (<sch:value-of select="document-uri(/)"/>).
                        </sch:assert>
      </sch:rule>
   </pattern>
</grammar>
