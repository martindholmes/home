   </define>
   <define name="xh_model.mhBlockBody">
      <choice>
         <ref name="xh_p"/>
         <ref name="xh_details"/>
         <ref name="xh_blockquote"/>
         <ref name="xh_ul"/>
         <ref name="xh_ol"/>
         <ref name="xh_pre"/>
         <ref name="xh_img"/>
         <ref name="xh_audio"/>
         <ref name="xh_video"/>
         <ref name="xh_dialog"/>
         <ref name="xh_cite"/>
         <ref name="xh_figure"/>
         <ref name="xh_button"/>
         <ref name="xh_table"/>
      </choice>
   </define>
   <define name="xh_att.hasLang.attributes">
      <ref name="xh_att.hasLang.attribute.lang"/>
   </define>
   <define name="xh_att.hasLang.attribute.lang">
      <optional>
         <attribute name="lang">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Specifies the language of the content of the element.) 
Suggested values include: 1] en (English); 2] fr (French); 3] it (Italian); 4] jp (Japanese); 5] zh (Chinese); 6] de (German); 7] la (Latin); 8] es (Spanish)</a:documentation>
            <choice>
               <value>en</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(English) </a:documentation>
               <value>fr</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(French) </a:documentation>
               <value>it</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Italian) </a:documentation>
               <value>jp</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Japanese) </a:documentation>
               <value>zh</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Chinese) </a:documentation>
               <value>de</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(German) </a:documentation>
               <value>la</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Latin) </a:documentation>
               <value>es</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Spanish) </a:documentation>
               <data type="token">
                  <param name="pattern">[^\p{C}\p{Z}]+</param>
               </data>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="xh_att.classable.attributes">
      <ref name="xh_att.classable.attribute.class"/>
      <ref name="xh_att.classable.attribute.style"/>
   </define>
   <define name="xh_att.classable.attribute.class">
      <optional>
         <attribute name="class">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The class attribute provides styles through rules in the CSS stylesheet.) </a:documentation>
            <list>
               <oneOrMore>
                  <data type="token">
                     <param name="pattern">[^\p{C}\p{Z}]+</param>
                  </data>
               </oneOrMore>
            </list>
         </attribute>
      </optional>
   </define>
   <define name="xh_att.classable.attribute.style">
      <optional>
         <attribute name="style">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The style attribute provides one-off style options for specific and unusual cases. Do not use this unless you know that the element you're styling is unlike any other element in the project.) </a:documentation>
            <data type="string"/>
         </attribute>
      </optional>
   </define>
   <define name="xh_att.identifiable.attributes">
      <ref name="xh_att.identifiable.attribute.id"/>
   </define>
   <define name="xh_att.identifiable.attribute.id">
      <optional>
         <attribute name="id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <data type="ID"/>
         </attribute>
      </optional>
   </define>
   <define name="xh_att.hasnametype.attributes">
      <ref name="xh_att.hasnametype.attribute.data-nametype"/>
   </define>
   <define name="xh_att.hasnametype.attribute.data-nametype">
      <optional>
         <attribute name="data-nametype">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <choice>
               <value>monographTitle</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Book or play title (will be italicized)) </a:documentation>
               <value>periodicalTitle</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Title of newspaper, magazine, or journal (will be italicized)) </a:documentation>
               <value>poemTitle</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Poem title (will be surrounded with quotes)) </a:documentation>
               <value>songTitle</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Song title (will be surrounded with quotes)) </a:documentation>
               <value>articleTitle</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Article title (will be surrounded with quotes)) </a:documentation>
               <value>persName</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The name of a person) </a:documentation>
               <value>forename</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The forename of a person, usually inside a persName.) </a:documentation>
               <value>surname</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(The surname of a person, usually inside a persName.) </a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="xh_att.hasHref.attributes">
      <ref name="xh_att.hasHref.attribute.href"/>
   </define>
   <define name="xh_att.hasHref.attribute.href">
