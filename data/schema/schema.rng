      <choice>
         <ref name="xh_summary"/>
         <ref name="xh_q"/>
         <ref name="xh_span"/>
         <ref name="xh_abbr"/>
         <ref name="xh_address"/>
         <ref name="xh_strong"/>
         <ref name="xh_code"/>
         <ref name="xh_img"/>
         <ref name="xh_audio"/>
         <ref name="xh_video"/>
         <ref name="xh_canvas"/>
         <ref name="xh_em"/>
         <ref name="xh_u"/>
         <ref name="xh_a"/>
         <ref name="xh_cite"/>
         <ref name="xh_figcaption"/>
