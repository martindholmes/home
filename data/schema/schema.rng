         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         <data type="anyURI">
            <param name="pattern">\S+</param>
         </data>
      </attribute>
   </define>
   <define name="xh_att.hasTitle.attributes">
      <ref name="xh_att.hasTitle.attribute.title"/>
   </define>
   <define name="xh_att.hasTitle.attribute.title">
      <optional>
         <attribute name="title">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
            <data type="string"/>
         </attribute>
      </optional>
   </define>
   <define name="xh_att.hasTabindex.attributes">
      <ref name="xh_att.hasTabindex.attribute.tabindex"/>
   </define>
   <define name="xh_att.hasTabindex.attribute.tabindex">
      <optional>
         <attribute name="tabindex">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
         </attribute>
      </optional>
   </define>
   <define name="xh_att.hasWidthAndHeight.attributes">
      <ref name="xh_att.hasWidthAndHeight.attribute.width"/>
      <ref name="xh_att.hasWidthAndHeight.attribute.height"/>
   </define>
   <define name="xh_att.hasWidthAndHeight.attribute.width">
      <optional>
         <attribute name="width">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Width in pixels of the image.</a:documentation>
            <choice>
               <data type="double"/>
               <data type="token">
                  <param name="pattern">(\-?[\d]+/\-?[\d]+)</param>
               </data>
               <data type="decimal"/>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="xh_att.hasWidthAndHeight.attribute.height">
      <optional>
         <attribute name="height">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Height in pixels of the image</a:documentation>
            <choice>
               <data type="double"/>
               <data type="token">
                  <param name="pattern">(\-?[\d]+/\-?[\d]+)</param>
               </data>
               <data type="decimal"/>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="xh_att.hasSpan.attributes">
      <ref name="xh_att.hasSpan.attribute.span"/>
   </define>
   <define name="xh_att.hasSpan.attribute.span">
      <optional>
         <attribute name="span">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Number of items such as columns spanned by the element bearing this attribute.</a:documentation>
            <choice>
               <data type="double"/>
               <data type="token">
                  <param name="pattern">(\-?[\d]+/\-?[\d]+)</param>
               </data>
               <data type="decimal"/>
            </choice>
         </attribute>
      </optional>
   </define>
